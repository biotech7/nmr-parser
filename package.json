{
  "name": "nmr-parser",
  "version": "0.0.1",
  "description": "Read and convert any NMR file",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "rollup -c",
    "test": "npm run test-coverage && npm run eslint",
    "test-coverage": "jest --coverage",
    "test-only": "jest",
    "build": "cheminfo-build --entry src/index.js --root NMRparser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/nmr-parser.git"
  },
  "keywords": [
    "nmr",
    "magnetic resonance",
    "parser",
    "bruker",
    "JEOL"
  ],
  "author": "Julien Wist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/nmr-parser/issues"
  },
  "homepage": "https://github.com/cheminfo/nmr-parser#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@rollup/plugin-json": "^4.0.3",
    "@types/jest": "^25.2.2",
    "cheminfo-build": "^1.1.1",
    "cheminfo-publish": "^1.0.10",
    "eslint": "^7.0.0",
    "eslint-config-cheminfo": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jeol-data-test": "0.1.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "rollup": "^2.10.0"
  },
  "dependencies": {
    "jeolconverter": "0.1.0"
  }
}
